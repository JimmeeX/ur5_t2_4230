<?xml version="1.0" ?>
<sdf version="1.6">
  <model name="conveyor_belt">

    <model name="conveyor_belt_fixed">
      <static>true</static>
      <pose>-1 0 0 0 0 0</pose>
      <link name="link">
        <pose>-1 0 0 0 0 0</pose>
       <visual name="belt_visual">
          <geometry>
            <mesh>
              <uri>model://conveyor_belt/meshes/conveyor.dae</uri>
              <scale>1 1 1</scale>
            </mesh>
          </geometry>
        </visual>
      </link>
    </model>

    <model name="conveyor_belt_moving">
      <static>false</static>
      <pose>-2 0 0.92 0 0 0</pose>
      <link name="belt">
        <pose>5 0 -0.003 0 0 0</pose>
        <inertial>
          <inertia>
            <ixx>3.8185</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>1781.5</iyy>
            <iyz>0</iyz>
            <izz>1784.72</izz>
          </inertia>
          <mass>100</mass>
        </inertial>
       <visual name="belt_collision">
          <geometry>
            <box>
              <size>10 0.6 0.006</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/DarkGrey</name>
            </script>
          </material>
        </visual>
        <collision name="belt_collision">
          <geometry>
            <box>
              <size>10 0.6 0.006</size>
            </box>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>1.0</mu>
                <mu2>1.0</mu2>
              </ode>
              <torsional>
                <coefficient>1000.0</coefficient>
                <patch_radius>0.1</patch_radius>
              </torsional>
            </friction>
          </surface>
        </collision>
      </link>
    </model>

    <joint name="belt_joint" type="prismatic">
      <parent>conveyor_belt_fixed::link</parent>
      <child>conveyor_belt_moving::belt</child>
      <axis>
        <xyz>1 0 0</xyz>
        <limit>
          <lower>0</lower>
          <upper>1.0</upper>
        </limit>
      </axis>
    </joint>

    <plugin name="conveyor_belt_plugin" filename="libROSConveyorBeltPlugin.so">
      <power>100.0</power>
      <!-- <robot_namespace>/ur5_t2_4230</robot_namespace> -->
      <!-- <population_rate_modifier_topic>/ur5_t2_4230/population/rate_modifier</population_rate_modifier_topic> -->
      <link>conveyor_belt::conveyor_belt_moving::belt</link>
      <max_belt_linear_vel>1.0</max_belt_linear_vel>
    </plugin>

  </model>
</sdf>